{
  "name": "expressapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "pkg": "pkg ."
  },
  "bin": "./bin/www",
  "pkg": {
    "assets": [
      "public/**/*",
      "views/**/*"
    ],
    "targets": [
      "win"
    ],
    "outputPath": "dist/",
    "options": [
      "experimental-modules"
    ]
  },
  "db": {
    "url": "mongodb://localhost:27017",
    "dbname": "firstNodeDB"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongodb": "^4.13.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.20"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "mongoose": "^6.9.0",
    "xmlhttprequest": "^1.8.0"
  }
}
